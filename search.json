---
---
[
  {% for post in site.posts %}
    {
      "title"    : {{ post.title | jsonify }},
      "url"      : {{ post.url | relative_url | jsonify }},
      "content"  : {{ post.content | strip_html | strip_newlines | remove: '  ' | jsonify }}
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]